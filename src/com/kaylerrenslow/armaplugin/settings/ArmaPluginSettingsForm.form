        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <vspacer id="f7ed5">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="2" use-parent-layout="false"/>
            </constraints>
          </vspacer>
          <hspacer id="d4151">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
          </hspacer>
